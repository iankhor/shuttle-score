h1
  font-size 2em



.court
  background-color #0074D9
  // border 2px solid #000
  padding 20px
  height 630px
  width 1400px

.courtline
  border 3px solid #FFF
  

.courtline-topside
  border 3px solid #FFF
  border-top 6px solid #FFF

.courtline-bottomside
  border 3px solid #FFF
  border-bottom 6px solid #FFF

.countline-midleft
  border-left 6px solid red

.countline-midright
  border-right 6px solid red

.courtline-left
  border-left 6px solid #FFF

.courtline-right
  border-right 6px solid #FFF

.courtline-leftside
  border 3px solid #FFF
  border-left 6px solid #FFF

.courtline-rightside
  border 3px solid #FFF
  border-right 6px solid #FFF

.courtline-horizontal-thick
  border-top 6px solid #FFF

.courtline-vertical-side
  border-top 6px solid #FFF

.sideline
  height 42px

.sideline-back
  width 80px
  height 42px
  display inline-block

.sideline-mid
  width 390px
  height 42px
  display inline-block

.sideline-front
  width 200px
  height 42px
  display inline-block

.midcourt
  height 253px

.midcourt-back
  width 80px
  height 253px
  display inline-block

.midcourt-mid
  width 390px
  height 253px
  display inline-block

.midcourt-front
  width 200px
  height 253px
